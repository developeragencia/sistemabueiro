    "build:copy": "cp -r dashboard/admin/* dist/ && rm -rf dist/js",
    "dev": "npm run dev:css && npm run dev:js",
    "dev:css": "sass --watch dashboard/admin/css/style.scss dashboard/admin/css/style.css",
    "dev:js": "webpack --mode development --watch",
